{
  "manifest_version": 3,
  "name": "Easemylink",
  "description": "save links with easy retrieval in the future",
  "version": "0.1.0",
  "action": {
    "default_title": "Easemylink",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}